<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageViewRing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/titlepad_ring_185x110" />
    
    <ImageView
        android:id="@+id/imageViewTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/titlepad_text_142x46" />
    
    <ImageView
        android:id="@+id/imageViewCross"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/titlepad_cross_50x50" />
    
    <ImageView
        android:id="@+id/imageViewResistor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/titlepad_resistor_21x70" />

</FrameLayout>
